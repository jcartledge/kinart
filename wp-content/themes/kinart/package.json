{
  "name": "kinart",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "prebuild:css": "node-sass --include-path=src/scss --source-map-embed src/scss/main.scss > style.css",
    "build:css": "postcss --use autoprefixer --autoprefixer.browsers 'last 2 versions' --use cssnano -o style.css style.css",
    "build:js": "browserify src/js/index.js -o bundle.js -t [ babelify --presets [ es2015 ] ]",
    "build:js:dev": "browserify src/js/index.js -o bundle.js -t [ babelify --presets [ es2015 ] ] -d",
    "build": "npm run build:css && npm run build:js",
    "build:dev": "npm run build:css && npm run build:js:dev",
    "dev": "watch 'npm run build:dev' src/*"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-preset-es2015": "^6.13.2",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "cssnano": "^3.6.2",
    "event-listener": "^1.1.0",
    "jquery": "^3.1.0",
    "node-sass": "^3.7.0",
    "once-event-listener": "^1.0.0",
    "postcss-cli": "^2.5.2",
    "watch": "^0.18.0"
  }
}
